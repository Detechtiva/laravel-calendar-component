{
    "name": "detechtiva/vue-calendar-for-laravel",
    "description": "Vue Calendar for Laravel",
    "keywords": [
        "detechtiva",
        "vue-calendar-for-laravel"
    ],
    "homepage": "https://github.com/detechtiva/vue-calendar-for-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Detechtiva",
            "email": "miguelbonifaz126@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.3",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Detechtiva\\VueCalendarForLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Detechtiva\\VueCalendarForLaravel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Detechtiva\\VueCalendarForLaravel\\VueCalendarForLaravelServiceProvider"
            ],
            "aliases": {
                "VueCalendarForLaravel": "Detechtiva\\VueCalendarForLaravel\\VueCalendarForLaravelFacade"
            }
        }
    }
}
